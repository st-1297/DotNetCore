@* モデルの型を宣言 *@
@model IEnumerable<QuickMaster.Models.Book>

<table class="table">
    <thead>
        <tr>
            <th>書名</th>
            <th>価格</th>
            <th>出版社</th>
        </tr>
    </thead>
    <tbody>
        @* 書籍情報を順に出力 *@
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Title</td>
                <td>@item.Price</td>
                <td>@item.Publisher</td>
            </tr>
        }
    </tbody>
</table>